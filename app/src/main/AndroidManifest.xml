<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coinmanage">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_coinmanage"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_coinmanage_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AddNoActivity"></activity>
        <activity
            android:name=".NoActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AddChiTieuActivity" />
        <activity
            android:name=".ChiTieuActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AddThuNhapActivity" />
        <activity
            android:name=".ThuNhapActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>